{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/api/main.js" },
      { "src": "/api/users", "dest": "/api/users.js" },
      { "src": "/api/tools", "dest": "/api/tools.js" }
    ]
  }